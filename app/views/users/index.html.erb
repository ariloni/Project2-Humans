<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li><%= user.first_name + " " +  user.last_name %></li> 
    
  <% end %>
</ul>

<% if current_user  %>

	You are logged in as <%= current_user.user_name %>
	<%= link_to current_user.user_name + "'s profile", posts_path %>
	<%= link_to 'LOG OUT!', logout_path, method: :delete %>
	<td><%= link_to 'Edit', edit_user_path(current_user) %></td>
	<%= link_to 'Destroy', delete_user_path(current_user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
	
<% else %>
	<%= link_to 'SIGN UP', users_new_path %>
	<%= link_to 'LOG IN', login_path %>
<% end %>